!!!
%html
  %head
    %title jpyammer
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    = yield :head
  %body
    .topbar
      = link_to 'Home', root_url
    .container-fluid
      #contens_flash
        - if notice.present?
          %p
            = notice
        - if alert.present?
          %p
            = alert
      #contens_main
        = yield

